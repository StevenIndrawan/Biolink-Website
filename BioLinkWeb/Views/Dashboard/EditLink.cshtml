@model BioLinkWeb.Models.Link
<h2>Edit Link</h2>
<form asp-action="EditLink" method="post">
    <input type="hidden" asp-for="Id" />
    <label>Title</label>
    <input asp-for="Title" />
    <label>URL</label>
    <input asp-for="Url" />
    <label>Icon</label>
    <input asp-for="Icon" />
    <label>Order</label>
    <input asp-for="Order" />
    <button type="submit">Update</button>
</form>
