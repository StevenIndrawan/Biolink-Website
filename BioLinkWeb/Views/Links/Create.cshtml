@model BioLinkWeb.Models.UserLink

<h2>Add New Link</h2>

<form asp-action="Create" method="post">
    <div>
        <label>Title</label>
        <input asp-for="Title" class="form-control"/>
    </div>
    <div>
        <label>URL</label>
        <input asp-for="Url" class="form-control"/>
    </div>
    <button type="submit" class="btn btn-primary mt-2">Save</button>
</form>
